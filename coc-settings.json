{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "args": ["-remote=auto"],
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        },
        "rust": {
          "command": "rust-analyzer",
          "filetypes": ["rust"],
          "rootPatterns": ["Cargo.toml"]
        }
    },
    "suggest.autoTrigger": "trigger"
}
